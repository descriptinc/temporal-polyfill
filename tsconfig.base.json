{
  "compilerOptions": {
    "strict": true,
    "module": "ES2020",
    "target": "ES2020",
    "lib": ["ES2020"],
    "moduleResolution": "Node",
    "composite": true,
    "allowJs": true,
    "isolatedModules": true,
    "emitDeclarationOnly": true,
    "esModuleInterop": true,
    "importHelpers": true,
    "resolveJsonModule": true,
    "declaration": true,
    "declarationMap": true,
    "skipLibCheck": false,
    "checkJs": false
  }
}
